pipeline{
    agent any
    stages{

        stage('git checkout'){

            steps{

                git branch: 'main', url: 'https://github.com/vaibhav9191/demo-counter.git'

            }


        }
        stage('unit testing'){

            steps{
                
                script{

                    sh 'mvn test'
    
                }

            }

            

        }
    }
}